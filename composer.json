{
	"name":"laemmi/contao-downloadarchive",
	"description":"Define downloadarchives with permission for users to download single files. fork von felixpfeiffer/contao-downloadarchive",
	"keywords":["download","archive","permission"],
	"type":"contao-module",
	"homepage":"http://www.felixpfeiffer.com/",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Felix Pfeiffer",
			"homepage":"http://www.felixpfeiffer.com/",
			"role":"Developer"
		},
		{
			"name":"Michael LÃ¤mmlein",
			"homepage":"https://www.spacerabbit.de/",
			"role":"Developer"
		}
	],
	"support":{
		"source":"https://github.com/laemmi/contao-downloadarchive"
	},
	"require":{
		"php": "^5.4|^7.0",
		"contao/core-bundle": "~3.5 || ~4.4",
		"contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
	},
	"autoload":{
		"classmap":[""]
	},
	"replace":{
		"contao-legacy/downloadarchive":"*"
	},
	"extra":{
		"contao":{
			"sources":{
				"":"system/modules/downloadarchive"
			}
		}
	},
	"config": {
		"platform": {
			"php": "7.4"
		},
		"allow-plugins": {
			"contao-components/installer": true,
			"contao-community-alliance/composer-plugin": true
		}
	}
}
